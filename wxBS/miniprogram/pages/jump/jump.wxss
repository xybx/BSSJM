/* pages/jump/jump.wxss */
page{
  background-color: #fff!important;
}
.iconbox{
  display: flex;
  flex-direction: column;
  width: 430rpx;
  height: 390rpx;
  margin: 0 auto;
  padding-top:240rpx;
}
.iconbox image{
  width: 100%;
  height: 100%;
}
.fontbox{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-top:100rpx;
}
.fontbox text{
  font-size:18px;
}
.fontbox text:first-child{
  color:#4E9DF4;
  margin-bottom: 10px;
  font-weight: 600;
}
.btnbox{
  width:300rpx;
  height:70rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 20px;
  border: 1px solid #4E9DF4;
  border-radius: 25px;
  color:#4E9DF4;
  margin-top: 100rpx;
}



